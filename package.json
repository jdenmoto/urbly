    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "firebase": "^10.14.1",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
